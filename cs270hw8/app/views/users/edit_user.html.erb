<%= form_for @user, url: update_user_path, method: 'post' do |f| %>
    <%= f.label :first_name, 'First Name' %>
    <%= f.text_field :first_name %>
    <br>
    <%= f.label :last_name, 'Last Name' %>
    <%= f.text_field :last_name %>
    <br>
    <%= f.label :email, 'Email' %>
    <%= f.text_field :email %>
    <br>
    <%= f.submit 'Submit' %>
<% end%>